package Intro;

public class MyInfo {

	public static void main(String[] args) {
		//이름을 저장할 변수 선언하여 본인이름 할당
		//나이를 저장할 변수 선언하여 본인나이 할당
		//키를 저장할 변수 선언하여 본인키 소숫점까지 할당
		//남여 구분을 저장할 변수를 선언하여 본인 성별 할당 'F' or'M'
		
		String name="박철현";
		String age="25";
		float h=171.1f;
		double h2=174.5;
		char g= 'M';
		System.out.println(String.format("%s, %s,%f,%f,%c",name, age,h,h2,g));
		System.out.println(String.format(age, args));
	}

}
