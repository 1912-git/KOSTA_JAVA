package Variable;

public class StringPlus {

	public static void main(String[] args) {
			int v1=10;		//int v1=10, v2=20;
			int v2=20;
			
			int res =v1+v2;
			System.out.println(res);
			
			String str1="abc";
			String str2="def";
			String sres= str1+str2;
			System.out.println(sres);
			
			System.out.println(str1+res);
			
			String seven1 ="7";
			char seven2= '7';
			int seven3=7;
			System.out.println(seven1+seven2+seven3);//문자열이 맨앞에 오면 전체다 문자열화
			System.out.println(seven2+seven3+seven1);//문자가 맨앞에오면 아스키코드로 바뀐후 숫자는 더함
			System.out.println(seven3+seven2+seven1);
			System.out.println(seven2+seven1);
			System.out.println("7"+7+7);
			System.out.println(7+7+"7");
			// 7의 아스키코드 55 A는 65 a 97
			
	}

}
